<Window x:Class="SQLDatabaseEdendesign.Add_Record"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Add_Record" Height="800" Width="300">
    <DockPanel Height="800" Width="275">
        <StackPanel Name="RecordStack" VerticalAlignment="Center" HorizontalAlignment="Center" DockPanel.Dock="Top">            
        </StackPanel>
        <Button Name="Save" Width="50" Height="25" Content="Save" DockPanel.Dock="Bottom" Click="Save_Click"></Button>
    </DockPanel>
</Window>
